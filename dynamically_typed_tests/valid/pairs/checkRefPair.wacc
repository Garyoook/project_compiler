# create a pair(int, char) with a second reference to it and check it works

# Output:
# #addrs#
# #addrs#
# true
# 10
# 10
# true
# a
# a
# true

# Program:

begin
  p = newpair(10, 'a') ;
  q = p;
  println p ;
  println q ;
  println p == q ;
  x = fst p ;
  y = fst q ;
  println x ;
  println y ;
  println x == y ;
  c1 = snd p ;
  c2 = snd q ;
  println c1 ;
  println c2 ;
  println c1 == c2
end
